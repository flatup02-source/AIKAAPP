      "typeVersion": 2,
      "position": [650, 300]
    },
    {
      "parameters": {
        "authentication": "accessToken",
        "resource": "message",
        "operation": "getContent",
        "messageId": "={{ $json.events[0].message.id }}",
        "options": {}
      },
      "id": "get-video",
      "name": "動画ファイルを取得",
      "type": "n8n-nodes-base.line",
      "typeVersion": 1,
      "position": [850, 300],
      "credentials": {
        "lineApi": {
          "id": "1",
          "name": "LINE Messaging API"
        }
      }
    },
    {
      "parameters": {
        "operation": "upload",
        "name": "={{ $json.events[0].message.id }}.mp4",
        "binaryData": true,
        "options": {}
      },
      "id": "upload-gcs",
      "name": "Google Cloud Storageにアップロード",
      "type": "n8n-nodes-base.googleCloudStorage",
      "typeVersion": 1,
      "position": [1050, 300],
      "credentials": {
        "googleCloudStorageOAuth2Api": {
          "id": "2",
          "name": "Google Cloud Storage"
        }
      }
    },
    {
      "parameters": {
        "url": "={{ $env.BACKEND_URL }}/api/analyze-video",
        "authentication": "none",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": []
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "gcsUri",
              "value": "={{ $json.selfLink }}"
            }
          ]
        },
        "options": {}
      },
      "id": "analyze-video",
      "name": "動画解析API呼び出し",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [1250, 300]
    },
    {
      "parameters": {
        "url": "={{ $env.BACKEND_URL }}/api/aika18/video-analysis",
        "authentication": "none",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": []
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "videoAnalysisResult",
              "value": "={{ $json }}"
            },
            {
              "name": "userId",
              "value": "={{ $('LINE Webhook').item.json.events[0].source.userId }}"
            }
          ]
        },
        "options": {}
      },
      "id": "call-aika18-video",
      "name": "AIKA18動画解析API呼び出し",
